{
  "name": "GitTrending",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {"128": "icon_128.png"},
  "minimum_chrome_version": "33",
  
  "browser_action": {
    "default_icon": "assets/flame.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "identity",
    "https://github.com/"
  ]
}